{
  "name": "deivamanikailasam",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "file-server": "node file-server.js",
    "dev": "concurrently \"npm run start\" \"npm run file-server\"",
    "watch": "ng build --watch --configuration development",
    "build": "ng build --configuration production --base-href=\"/deivamanikailasam/\"",
    "json-server": "json-server --watch db.json --port 3000",
    "test": "ng test",
    "lint": "ng lint",
    "deploy": "npm run build && npx angular-cli-ghpages --dir=dist/deivamanikailasam/browser"
  },
  "prettier": {
    "printWidth": 100,
    "singleQuote": true,
    "overrides": [
      {
        "files": "*.html",
        "options": {
          "parser": "angular"
        }
      }
    ]
  },
  "private": true,
  "dependencies": {
    "@angular/common": "^20.3.0",
    "@angular/compiler": "^20.3.0",
    "@angular/core": "^20.3.0",
    "@angular/forms": "^20.3.0",
    "@angular/platform-browser": "^20.3.0",
    "@angular/router": "^20.3.0",
    "@primeuix/themes": "^1.2.5",
    "bootstrap": "^5.3.8",
    "primeicons": "^7.0.0",
    "primeng": "^20.3.0",
    "quill": "^2.0.3",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0"
  },
  "devDependencies": {
    "@angular/build": "^20.3.8",
    "@angular/cli": "^20.3.8",
    "@angular/compiler-cli": "^20.3.0",
    "@types/jasmine": "~5.1.0",
    "concurrently": "^9.2.1",
    "cors": "^2.8.5",
    "express": "^4.21.2",
    "jasmine-core": "~5.9.0",
    "json-server": "^1.0.0-beta.3",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "typescript": "~5.9.2"
  }
}
